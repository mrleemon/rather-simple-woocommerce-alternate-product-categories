{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "occ/alternate-product-categories",
  "title": "Alternate Product Categories",
  "description": "An alternate product categories block.",
  "textdomain": "rather-simple-woocommerce-alternate-product-categories",
  "category": "widgets",
  "attributes": {
    "dropdown": {
      "type": "boolean",
      "default": false
    },
    "count": {
      "type": "boolean",
      "default": false
    }
  },
  "supports": {
    "anchor": true,
    "html": false,
